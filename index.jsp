            <input type="button" id="botones" value="2" onclick="anotar('2');" />
            <input type="button" id="botones" value="1" onclick="anotar('1');" /><br />
            <input type="button" id="botones" value="/ " onclick="opera('/');" />
            <input type="button" id="botones" value="0" onclick="anotar('0');" />
